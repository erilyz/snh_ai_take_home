# Tree Management API Configuration

# Storage Backend (local, gcs, or s3)
STORAGE_TYPE=local

# Local Storage Configuration
STORAGE_PATH=data/trees.json

# Google Cloud Storage Configuration (if STORAGE_TYPE=gcs)
# GCS_BUCKET_NAME=your-bucket-name
# GCS_OBJECT_NAME=trees.json
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# AWS S3 Configuration (if STORAGE_TYPE=s3)
# S3_BUCKET_NAME=your-bucket-name
# S3_OBJECT_KEY=trees.json
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key

# Logging
LOG_LEVEL=INFO

